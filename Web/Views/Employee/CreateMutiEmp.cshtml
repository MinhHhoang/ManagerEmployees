@model DomainModel.Employee
@{
     ViewBag.Title = "Nhập dữ liệu nhiều nhân viên";
}

<div class="box box-primary">
    <div class="box-header">
    </div>

    <div class="box-body">
        <form class="form-horizontal" action="~/employee/ImportFile" method="post" enctype="multipart/form-data">
           

            <div class="form-group">
                <label class="control-label col-sm-2">File import</label>
                <div class="col-sm-10">
                    <input type="file" class="form-control" name="fileExcel" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
                    <span style="margin-top:20px;color:red"> @ViewBag.Error</span>
                </div>
            </div>

            <div class="form-group" style="margin-top:20px">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-primary">
                        <i class="fa fa-save"></i> Import
                    </button>
                    <a class="btn btn-warning" href="~/employee">
                        <i class="fa fa-ban"></i> Quay lại
                    </a>
                </div>
            </div>
        </form>
    </div>
    <div class="box-footer text-center">
    </div>

</div>